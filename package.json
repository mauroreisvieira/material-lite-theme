{
    "name": "sublime-meetio-theme",
    "version": "4.5.3",
    "description": "The most powerful and adaptive theme for Sublime Text 3",
    "bugs": {
        "url": "https://github.com/meetio-theme/sublime-meetio-theme/issues"
    },
    "author": "Mauro Reis Vieira <mauroreisvieira@gmail.com>",
    "license": "MIT",
    "keywords": [
        "sublime-text",
        "scheme",
        "theme"
    ],
    "scripts": {
        "theme": "ts-node src/theme.ts",
        "scheme": "ts-node src/scheme.ts",
        "icons": "ts-node src/icons.ts",
        "lint": "eslint \"src/*.ts\" \"src/**/*.ts\" \"src/**/**/*.ts\" \"src/**/*.json\" \"src/**/**/*.json\"",
        "changelog": "conventional-changelog -p angular -r 2 -i CHANGELOG.md -s",
        "release": "bash scripts/release.sh",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "engines": {
        "node": "^11.0.0"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "dependencies": {
        "@types/node": "^13.13.5"
    },
    "devDependencies": {
        "@babel/core": "7.9.6",
        "@babel/preset-env": "7.9.6",
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^8.3.4",
        "conventional-changelog-cli": "^2.0.11",
        "@meetio/meetio-colors": "^1.7.0",
        "@meetio/meetio-icons": "^1.10.0",
        "@meetio/scheme-generator": "^1.10.2",
        "@typescript-eslint/eslint-plugin": "2.32.0",
        "@typescript-eslint/parser": "2.32.0",
        "babel-eslint": "^10.1.0",
        "eslint": "^7.0.0",
        "eslint-plugin-json": "^2.1.1",
        "glob": "^7.1.6",
        "husky": "^4.2.5",
        "prettier": "^2.0.5",
        "rimraf": "^3.0.2",
        "svg2img": "^0.6.1",
        "ts-node": "^8.10.1",
        "typescript": "^3.8.3"
    }
}
